<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.missmess.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SwipeListViewActivity">
        </activity>
        <activity android:name=".SwipeRecyclerViewActivity">
        </activity>
        <activity android:name=".SwipeGridViewActivity">
        </activity>
        <activity android:name=".SwipeExpandableListActivity">
        </activity>
        <activity android:name=".CustomLoadFactoryActivity">
        </activity>
        <activity android:name=".NoneRecyclerViewActivity"/>
        <activity android:name=".SmartRefreshRecyclerViewActivity"/>
    </application>

</manifest>